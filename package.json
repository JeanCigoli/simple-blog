    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "prebuild": "rimraf build dist",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "server:dev": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/main/server.ts",
    "server:debug": "tsnd -r tsconfig-paths/register --inspect --transpile-only --poll --watch --clear --ignore-watch node_modules src/main/server.ts",
    "server:start": "node dist/main/server.js"
  },
